def insertion_sort(a):
    for i in range(1, len(a)-1):
        key = a[i]          
        j = i - 1
        
        while j >= 0 and a[j] > key:
            a[j + 1] = a[j]
            j -= 1
        a[j + 1] = key     
    return a

a = [12, 5, 7, 3, 15, 1, 9]
print(insertion_sort(a))



time complexity :O(n^2) 
space complexity :O(n^2) 



